package com.A.enums;

public enum LineItemStatusCodes {

	PromotionStatusChangeNotAllowed(789L, "Promotion Lineitem status cannot be changed.");
	
	private Long code;
	private String desc;
	
	LineItemStatusCodes(Long code, String desc) {
	
		this.code = code;
		this.desc = desc;
	}

	public Long getCode() {
		return code;
	}

	public void setCode(Long code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	
}

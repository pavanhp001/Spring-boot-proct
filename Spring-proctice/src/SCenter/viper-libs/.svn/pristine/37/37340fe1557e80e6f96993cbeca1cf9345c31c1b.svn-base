package com.ac.V.dao.broadcast;

import java.util.Map;

public class BroadcastMessage {

    private String id;
    private String broadcastMsg;
    private Map<String,String> messageHeaders;

    public BroadcastMessage (String id,String broadcastMsg, Map<String,String> headers) {
	this.id = id;
	this.broadcastMsg = broadcastMsg;
	this.messageHeaders = headers;
    }

    public String getBroadcastMsg() {
        return broadcastMsg;
    }
    public void setBroadcastMsg(String broadcastMsg) {
        this.broadcastMsg = broadcastMsg;
    }
    public Map<String, String> getMessageHeaders() {
        return messageHeaders;
    }
    public void setMessageHeaders(Map<String, String> messageHeaders) {
        this.messageHeaders = messageHeaders;
    }
    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }
    @Override
    public String toString() {
	return "BroadcastMessage [id=" + id + ", broadcastMsg=" + broadcastMsg + ", messageHeaders=" + messageHeaders + "]";
    }




}

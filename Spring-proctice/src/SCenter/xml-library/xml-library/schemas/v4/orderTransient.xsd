<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified" targetNamespace="http://xml.A.com/v4"
    xmlns:ac="http://xml.A.com/v4" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:comm="http://xml.A.com/common">

    <xs:include schemaLocation="order.xsd" />
    <xs:include schemaLocation="types.xsd" />
    <xs:include schemaLocation="product.xsd" />
    <xs:import schemaLocation="common.xsd" namespace="http://xml.A.com/common" />

    <xs:complexType name="transientRequestContainerType">
        <xs:sequence>
            <xs:element name="transientRequest" type="ac:transientRequestType"
                minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="transientResponseContainerType">
        <xs:sequence>
            <xs:element name="transientResponse" type="ac:transientResponseType"
                minOccurs="0" maxOccurs="1" />

            <xs:element name="lineItemId" type="xs:string"
                minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="transientRequestType">
        <xs:annotation>
            <xs:documentation>
                Data used to process an order but not part of an
                order. Request.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:annotation>
                <xs:documentation>
                    list of available phone numbers
                </xs:documentation>
            </xs:annotation>
            <xs:element name="providerRequestContainer" type="ac:ProviderRequestContainer"
                minOccurs="0"/>
            <xs:element name="action" type="xs:string" minOccurs="0"
                maxOccurs="1" />
            <xs:element name="lineItemNumber" type="xs:int" minOccurs="0"
                maxOccurs="1" />
            <xs:element name="requestedPhone" type="ac:valuelist"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="promotionId" type="xs:string" minOccurs="0"
                maxOccurs="unbounded" />
            <xs:element name="tvSelect" type="ac:TvSelect" minOccurs="0"
                maxOccurs="1" />
            <xs:element name="securityInfo" type="ac:SecurityInfo"
                minOccurs="0" />
            <xs:element name="availableConnections" type="ac:AvailableConnections"
                minOccurs="0" />
            <xs:element name="workOrderNote" type="ac:WorkOrderNote"
                minOccurs="0" />
            <xs:element name="accountNote" type="ac:AccountNote"
                minOccurs="0" />
            <xs:element name="billLanguage" type="ac:BillLanguage"
                minOccurs="0" />
            <xs:element name="webClaimId" type="xs:string" minOccurs="0" />
            <xs:element name="sellerId" type="ac:SellerId" minOccurs="0" />
            <xs:element name="billingCycleDay" type="xs:int"
                minOccurs="0" />
            <xs:element name="dISHCinemaSelected" type="xs:boolean"
                minOccurs="0" />
            <xs:element name="conversationId" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="completeCreditCheck" type="xs:boolean"
                minOccurs="0" />
            <xs:element name="NewsletterSelected" type="xs:boolean"
                minOccurs="0" nillable="true" />
            <xs:element name="scheduleDate" type="ac:ScheduleDateType"
                minOccurs="0" />           
            <xs:element name="customerCode" type="xs:string" minOccurs="0" />
            <xs:element name="termsAndConditions" type="ac:TermsAndConditionsType" minOccurs="0" />
            <xs:element name="dishEquipmentSolutionSelectionName" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="dishPromotionId" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="dishProgrammingValidFlag" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="dishConfigurationName" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="tvConstraints" type="ac:TvConstraints"
                minOccurs="0" />
			<xs:element name="receiversSolution" type="ac:ReceiversSolution"
                minOccurs="1"/>
            <xs:element name="creditQualificationFlag" type="xs:boolean"
                minOccurs="0" />
             <xs:element name="leadId" type="xs:string"
                minOccurs="0" />
             <xs:element name="promotionInfo" type="ac:PromoDescription" minOccurs="0"
                maxOccurs="unbounded" />
             <xs:element name="energyProduct" type="ac:EnergyProduct" minOccurs="0" maxOccurs="1"/>
             <xs:element name="lineItems" type="ac:lineItemCollectionType" minOccurs="1" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>
   
    <xs:complexType name="transientResponseType">
        <xs:annotation>
            <xs:documentation>
                Data used to process an order but not part of an
                order. Response
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:annotation>
                <xs:documentation>
                    Suggested Phone Numbers from the provider.
                    Confirmed Phone number from the provider.
                    List of products available
                    from the provider.
                </xs:documentation>
            </xs:annotation>
            <xs:element name="providerResponseContainer" type="ac:ProviderResponseContainer"
                minOccurs="0"/>
            <xs:element name="orderNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="suggestedPhone" type="ac:valuelist"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="confirmedPhone" type="ac:valuelist"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="product" type="ac:orderLineItemDetailTypeType"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="qualificationFlag" type="xs:boolean"
                minOccurs="0" />
            <xs:element name="programmingValidFlag" type="xs:boolean"
                minOccurs="0" />
            <xs:element name="customerType" type="xs:string"
                minOccurs="0" />
            <xs:element name="dishPromotionId" type="xs:string" minOccurs="0" maxOccurs="1" />

            <xs:element name="conversationId" type="xs:string" minOccurs="0" maxOccurs="1" />

            <xs:element name="productInfo" type="ac:ProductInfo"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="tvConstraints" type="ac:TvConstraints"
                minOccurs="0" />
            <xs:element name="receiver" type="ac:Receiver" minOccurs="0"
                maxOccurs="unbounded" />
            <xs:element name="priceInfo" type="ac:PriceInfo"
                minOccurs="0" />
            <xs:element name="dISHConfiguration" type="xs:string"
                minOccurs="0" />
            <xs:element name="creditQualResponse" type="ac:CreditQualResponseInfo"
                minOccurs="0" maxOccurs="1"/>
            <xs:element name="orderQualResponse" type="ac:OrderQualResponseInfo"
                minOccurs="0" maxOccurs="1"/>
            <xs:element name="submitOrderResponse" type="ac:SubmitOrderResponseInfo"
                minOccurs="0" maxOccurs="1"/>
            <xs:element name="scheduleDate" type="ac:ScheduleDateType"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="providerLineItemStatus" type="ac:providerLineItemStatusType"
                minOccurs="1" maxOccurs="1" />
            <xs:element name="termsAndConditions" type="ac:TermsAndConditionsType"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="customerOrderInfo" type="ac:CustomerOrderInfo"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="equipmentBundleInfo" type="ac:EquipmentBundleInfo"
                minOccurs="0" maxOccurs="1" />
             <xs:element name="receiversSolution" type="ac:ReceiversSolution"
                minOccurs="1"/>
            <xs:element name="equipmentSolutionInfo" type="ac:EquipmentSolutionInfo"
                minOccurs="1" maxOccurs="unbounded" />
            <xs:element name="productBundleInfo" type="ac:ProductBundleInfo"
                minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="authenticateCustomerResponse" type="ac:AuthenticateCustomerResponseInfo"
                minOccurs="0" maxOccurs="1"/>
            <xs:element name="rtimErrorCode" type="xs:string"
                minOccurs="0" />
            <xs:element name="alternatePromotions" type="ac:AlternatePromotion"
                minOccurs="0"  maxOccurs="1"/>
			<xs:element name="leadId" type="xs:string"
                minOccurs="0" />
             <xs:element name="receiverSelectionValid" type="xs:boolean"
                minOccurs="0" />
             <xs:element name="promotionInfo" type="ac:PromoDescription" minOccurs="0"
                maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CustomerOrderInfo">
        <xs:annotation>
            <xs:documentation>
                ATT STI Customer CreditInfo, Disposition and Product Scheduling Information.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="OrderCustomizations" type="ac:customizationType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Disposition" type="ac:Disposition" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ProductSchedule" type="ac:ProductSchedule" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CreditDetails">
        <xs:sequence>
            <xs:element name="CreditDetail" type="ac:CreditDetail" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CreditDetail">
        <xs:sequence>
            <xs:element name="CustomizationCode" type="xs:string" minOccurs="1"/>
            <xs:element name="Name" type="xs:string" minOccurs="0"/>
            <xs:element name="ShortDescription" type="xs:string" minOccurs="0"/>
            <xs:element name="LongDescription" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupName" type="xs:string" minOccurs="0"/>
            <xs:element name="Schedule" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Choices" type="ac:CreditOptions" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="MinChoices" type="xs:nonNegativeInteger" minOccurs="0"/>
            <xs:element name="MaxChoices" type="xs:nonNegativeInteger" minOccurs="0"/>
            <xs:element name="CustomizationType" type="xs:string" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="required" type="xs:boolean" use="required"/>
    </xs:complexType>

    <xs:complexType name="CreditOptions">
        <xs:sequence>
            <xs:element name="ChoiceCode" type="xs:string"/>
            <xs:element name="Name" type="xs:string"/>
            <xs:element name="USOC" type="xs:string"/>
            <xs:element name="PaymentType" type="ac:PaymentUniqueId" minOccurs="0"/>
            <xs:element name="ShortDescription" type="xs:string" minOccurs="0"/>
            <xs:element name="LongDescription" type="xs:string" minOccurs="0"/>
            <xs:element name="Fees" type="ac:CreditFees" minOccurs="0"/>
            <xs:element name="ChoiceType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="OptionalPackage"/>
                        <xs:enumeration value="Equipment"/>
                        <xs:enumeration value="Credit"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="CreditDetails" type="ac:CreditDetails" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="PaymentUniqueId">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ADVANCED"/>
            <xs:enumeration value="AUTO"/>
            <xs:enumeration value="DEPOSIT"/>
            <xs:enumeration value="ONETIME"/>
            <xs:enumeration value="ONETIME_DTV"/>
            <xs:enumeration value="RECURRING"/>
            <xs:enumeration value="OUTSTANDING_BALANCE"/>
            <xs:enumeration value="EQUIPMENT"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="CreditFees">
        <xs:sequence>
            <xs:element name="Fee" type="xs:decimal"/>
            <xs:element name="FeeTerm" minOccurs="0" type="xs:string"/>
            <xs:element name="Description" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="ProductCategory">
        <xs:restriction base="xs:string">
            <xs:enumeration value="LocalPhone"/>
            <xs:enumeration value="DSL"/>
            <xs:enumeration value="DTV"/>
            <xs:enumeration value="Wireless"/>
            <xs:enumeration value="Fiber"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="Disposition">
        <xs:sequence>
            <xs:element name="MasterState" type="ac:OrderState" maxOccurs="1" minOccurs="1"/>
            <xs:element name="State" type="ac:OrderState" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ProductSchedule">
        <xs:sequence>
            <xs:element name="Scheduling" type="ac:ScheduleDateType" minOccurs="0"/>
            <xs:element name="Appointments" type="ac:AvailableAppointmentType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="productType" type="ac:ProductCategory" use="optional"/>
        <xs:attribute name="lineItem" type="xs:string" use="optional"/>
        <xs:attribute name="installationDate" type="xs:string" use="optional"/>
        <xs:attribute name="activationDate" type="xs:string" use="optional"/>
        <xs:attribute name="disconnectDate" type="xs:string" use="optional"/>
        <xs:attribute name="attOrderNumber" type="xs:string" use="optional"/>
        <xs:attribute name="subOrderNumber" type="xs:string" use="optional"/>
        <xs:attribute name="TnOrFtn" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:complexType name="OrderState">
        <xs:attribute name="whenAffected" type="xs:dateTime" use="required"/>
        <xs:attribute name="categoryId" type="xs:string" use="required"/>
        <xs:attribute name="state" type="xs:string" use="required"/>
        <xs:attribute name="reasonCode" type="xs:string" use="optional"/>
        <xs:attribute name="message" type="xs:string" use="optional"/>
        <xs:attribute name="subkey" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:complexType name="providerLineItemStatusType">
        <xs:sequence>
            <xs:element name="reason" type="xs:string" minOccurs="1"
                maxOccurs="unbounded" />

            <xs:element name="processingStatusCode" minOccurs="1"
                maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="INFO" />
                        <xs:enumeration value="ERROR" />
                        <xs:enumeration value="SUCCESS" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="lineItemStatusCode" minOccurs="0"
                maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="sales_new_order" />
                        <xs:enumeration value="processing_schedule_pending" />
                        <xs:enumeration value="processing_schedule_confirmed" />
                        <xs:enumeration value="processing_connected" />
                        <xs:enumeration value="processing_disconnected" />
                        <xs:enumeration value="processing_cancelled" />
                        <xs:enumeration value="hold_order_pending_problem" />
                        <xs:enumeration value="hold_authorization_pending" />
                        <xs:enumeration value="hold_provider" />
                        <xs:enumeration value="order_info_required" />
                        <xs:enumeration value="order_ready_for_submit" />
                        <xs:enumeration value="order_submitted" />
                        <xs:enumeration value="order_failed" />
                        <xs:enumeration value="failed" />
                        <xs:enumeration value="success" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dateTimeStamp" type="xs:dateTime" />
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="SellerId">
        <xs:annotation>
            <xs:documentation>DISH SellerId of the client. Should be not be used
                unless your implementation is a special scenario.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]+" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BillLanguage">
        <xs:annotation>
            <xs:documentation>Set to "S" to receive customer's bill in Spanish.
                Default is English.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[ES]" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="WorkOrderNote">
        <xs:annotation>
            <xs:documentation>Note that will be added to the Installation
                WorkOrder for the installer.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="180" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AccountNote">
        <xs:annotation>
            <xs:documentation>Note that will be added to the account and will be
                viewable by DISH customer service representatives.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="300" />
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="AvailableConnections">
        <xs:annotation>
            <xs:documentation>List of available connections that the customer
                has.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="connectionDetails" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Customer Connection</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="connectionType">
                            <xs:annotation>
                                <xs:documentation>Type of connection the customer has.
                                </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="NONE" />
                                    <xs:enumeration value="BROADBAND" />
                                    <xs:enumeration value="LANDLINEPHONE" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="provider" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Company/Provider name of connection.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SecurityInfo">
        <xs:sequence>
            <xs:element name="PIN">
                <xs:annotation>
                    <xs:documentation>4-digit numeric value used to verify ownership of
                        account for further access.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[0-9]{4}" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="PINHint">
                <xs:annotation>
                    <xs:documentation>200-character alpha-numeric value that alludes to
                        the PIN to assist user access.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="0" />
                        <xs:maxLength value="50" />
                        <xs:whiteSpace value="replace" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="AuthorizedPersons" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Names of users customer wishes to allow access to
                        account.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="name" type="comm:Name" maxOccurs="3" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ProductInfo">
        <xs:sequence>
            <xs:element name="productId" type="xs:int" />
            <xs:element name="productRule" type="ac:ProductRule" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PriceInfo">
        <xs:sequence>
            <xs:element name="priceCategory" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="categoryName" type="xs:string" />
                        <xs:element name="categoryTotal" type="xs:string" />
                        <xs:element name="productInfo" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="productName" type="xs:string" />
                                    <xs:element name="price" type="xs:string" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
             <xs:element name="partialPaymentDueOption" type="ac:PartialPaymentDueOption"
                minOccurs="0" />
            <xs:element name="amountDueNow" type="xs:string" />
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="PartialPaymentDueOption">
        <xs:sequence>
            <xs:element name="amountDueNow" type="xs:float" />
            <xs:element name="promptMessage" type="xs:string" />
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="ScheduleTimeSlots">
        <xs:sequence>
            <xs:element name="startHour" type="xs:time"/>
            <xs:element name="endHour" type="xs:time"/>
            <xs:element name="timeSlotRefId" type="xs:string" minOccurs="0"/>
            <xs:element name="isPremium" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ScheduleDateType">
        <xs:annotation>
            <xs:documentation>This element contains an install date
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="installDate" type="xs:date" />
            <xs:element name="activationDate" type="xs:date" minOccurs="0"/>
            <xs:element name="disconnectDate" type="xs:date" minOccurs="0"/>
            <xs:element name="installTimeSlot" type="ac:ScheduleTimeSlots"  maxOccurs="unbounded" />
            <xs:element name="activationTimeSlot" type="comm:TimeSlots"  maxOccurs="unbounded" minOccurs="0"/>
            <xs:element name="disconnectTimeSlot" type="comm:TimeSlots"  maxOccurs="unbounded" minOccurs="0"/>
            <xs:element name="timeDesc" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>MORNING or AFTERNOON</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="hasPremiumTimesslots" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AvailableAppointmentType">
        <xs:annotation>
            <xs:documentation>Appointment information for orders</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Appointment" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="apptStartDateTime" type="xs:date" />
                        <xs:element name="apptEndDateTime" type="xs:date" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TermsAndConditionsType">
        <xs:annotation>
            <xs:documentation>This element contains an install date
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="sessionID" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="termsAndConditions" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TvConstraints">
        <xs:annotation>
            <xs:documentation>This element lists the constraints as to how a
                customer may select TV's.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="maxNumTvs" type="xs:nonNegativeInteger" />
            <xs:element name="slingSupported" type="xs:boolean" />
            <xs:element name="wholeHomeOption" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="hdProgrammingPresent" type="xs:boolean"  minOccurs="0" maxOccurs="1" />
            <xs:element name="tvLabels">
                <xs:annotation>
                    <xs:documentation>This element lists all of the allowable TV
                        labels, including the default (i.e. which label to apply to each
                        TV initially -- and the only label that may be applied to more
                        than one TV).</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Default" type="xs:string" />
                        <xs:element name="Standard" type="xs:string" minOccurs="0"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="receiverOptions" type="ac:ReceiverOptions"  minOccurs="0" maxOccurs="1" />
         </xs:sequence>
      </xs:complexType>

      <xs:complexType name="ReceiverOptions">
        <xs:sequence>
          <xs:element name="receiverOption" type="ac:ReceiverOption"  minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:complexType>

      <xs:complexType name="ReceiverOption">

<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified"
	elementFormDefault="qualified" targetNamespace="http://xml.A.com/v4"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ac="http://xml.A.com/v4">

	<xs:include schemaLocation="payment.xsd" />
	<xs:include schemaLocation="address.xsd" />
	<xs:include schemaLocation="baseTypes.xsd" />

	<xs:complexType name="statusType">
		<xs:sequence>
			<xs:element name="statusCode" type="xs:int" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						A value that represents the high-level outcome of
						the request
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="processingMessages">
					<xs:annotation>
						<xs:documentation>
							A set of message elements that each contain a
							code and related
							explanation text.
						</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="message" type="ac:processingMessage"
								minOccurs="0" maxOccurs="unbounded" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="statusMsg" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							A message describing in text the same
							information encoded in the
							status code.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="processingMessage">
		<xs:sequence>
			<xs:element name="code" type="xs:double" />
			<xs:element name="text" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="orderStatusHistoryType">
		<xs:sequence>
			<xs:element name="previousStatus" type="ac:orderStatusWithTypeType"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="orderStatusWithTypeType">
		<xs:sequence>
			<xs:element name="status" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="reason" type="xs:int" minOccurs="1"
				maxOccurs="unbounded" />
			<xs:element name="dateTimeStamp" type="xs:dateTime"
				minOccurs="1" maxOccurs="1" />

			<xs:element name="agentId" type="xs:string" minOccurs="1"
				maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="notificationEventCollectionType">
		<xs:sequence>
			<xs:element name="event" type="ac:notificationEventType"
				minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="notificationEventType">
		<xs:sequence>
			<xs:element name="code" type="xs:int" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="reason" type="xs:int" minOccurs="1"
				maxOccurs="unbounded" />
			<xs:element name="comment" type="xs:string" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="acknowledged" type="xs:boolean" default="false"
				minOccurs="1" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="uniqueId" type="xs:string" />
		<xs:attribute name="dateTimeStamp" type="xs:dateTime" />
		<xs:attribute name="source" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="lineItemStatusType">

		<xs:sequence>
			<xs:element name="reason" type="xs:int" minOccurs="1"
				maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="statusCode" type="ac:lineItemStatusCodesType" />
		<xs:attribute name="statusCodeDescription" />
		<xs:attribute name="dateTimeStamp" type="xs:dateTime" />
		<xs:attribute name="agentId" type="xs:string" />
	</xs:complexType>

	<xs:simpleType name="lineItemStatusCodesType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="cancelled_removed" />
			<xs:enumeration value="hold_authorization_pending" />
			<xs:enumeration value="hold_customer_action" />
			<xs:enumeration value="hold_order" />
			<xs:enumeration value="hold_order_pending_problem" />
			<xs:enumeration value="hold_provider" />
			<xs:enumeration value="processing_aged" />
			<xs:enumeration value="processing_schedule_pending" />
			<xs:enumeration value="processing_schedule_confirmed" />
			<xs:enumeration value="processing_cancelled" />
			<xs:enumeration value="processing_connected" />
			<xs:enumeration value="processing_disconnected" />
			<xs:enumeration value="processing_problem_ordered_vru_failed" />
			<xs:enumeration value="sales_new_order" />
			<xs:enumeration value="sales_pre_order" />
			<xs:enumeration value="sales_submitted" />
			<xs:enumeration value="provision_ready" />
			<xs:enumeration value="provision_problem" />
			<xs:enumeration value="test_order" />
			<xs:enumeration value="sales_defer" />
			<xs:enumeration value="submit_failed" />
			<xs:enumeration value="reverse_processing_aged" />
			<xs:enumeration value="reverse_processing_schedule_pending" />
			<xs:enumeration value="reverse_processing_schedule_confirmed" />
			<xs:enumeration value="reverse_processing_connected" />
			<xs:enumeration value="reverse_processing_disconnected" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="lineItemStatusHistoryType">
		<xs:sequence>
			<xs:element name="previousStatus" type="ac:lineItemStatusType"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ccpCommunicationEventsType">
		<xs:sequence>
			<xs:element name="communicationEvent" type="ac:ccpCommunicationEventType" minOccurs="0"
						maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ccpCommunicationEventType">
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="1" maxOccurs="1" />
			<xs:element name="communicationAddress" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="communicationDate" type="xs:dateTime" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="communicationType" type="xs:string" minOccurs="1" maxOccurs="1" />
			<xs:element name="communicationStatus" minOccurs="1" maxOccurs="1" >
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="successful" />
						<xs:enumeration value="failed" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ccpEventNotificationType">
		<xs:sequence>
			<xs:element name="updateEmailInfo" type="xs:string"
				minOccurs="0" maxOccurs="1" />
			<xs:element name="orderID" type="xs:string"
				minOccurs="0" maxOccurs="1" />
			<xs:element name="bounceDate" type="xs:date" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="bounceType" minOccurs="0" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="hardBounce" />
						<xs:enumeration value="softBounce" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="clickDate" type="xs:date" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="clickUrl" type="xs:string" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="emailID" type="xs:int" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="emailStatus" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="emailStatus1" />
						<xs:enumeration value="emailStatus2" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="emailType" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<!-- ORDER_SUMM - 1 -->
						<xs:enumeration value="1" />

						<!-- STAT - 2 -->
						<xs:enumeration value="2" />

						<!-- ORP - 3 -->
						<xs:enumeration value="3" />

						<!-- PROVISIONING - 4 -->
						<xs:enumeration value="4" />

						<!-- ADHOC - 5 -->
						<xs:enumeration value="5" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="emailSegmentID" type="xs:int" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="emailStatusIDEncrypt" type="xs:string" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="openDate" type="xs:date" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="returnCode" type="xs:int" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="returnMsg" type="xs:string" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="sendDate" type="xs:date" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="custExternalID" type="xs:long" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="custExternalIDEncrypt" type="xs:string" minOccurs="0"
				maxOccurs="1" />

		</xs:sequence>

	</xs:complexType>

	<xs:complexType name="coreMarketItemType">
		<xs:sequence>
			<xs:element name="externalId" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="channels" type="ac:channelType"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="accordChangeStamp" type="xs:long"
				minOccurs="0" maxOccurs="1" />
			<xs:element name="accordPlanId" type="xs:int" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="serviceabilitySource" minOccurs="0"
				maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="internal" />
						<xs:enumeration value="realtime" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="providerId" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="providerName" type="xs:string"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The name of the provider of the marketItem. This
						is separate from the name in the associated item,
						since some
						vendors have different names depending
						on the market they are
						offering services in.
						This mostly happens in
						relation to a parent
						provider
						and its subsidiaries, i.e. AT&amp;T and BellSouth.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceInfo" type="ac:priceInfoType"
				minOccurs="1" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="marketItemType">
		<xs:complexContent>
			<xs:extension base="ac:coreMarketItemType">
				<xs:sequence>
					<xs:element name="item" type="ac:itemType" minOccurs="1"
						maxOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="marketItemWithCapabilitiesType">
		<xs:complexContent>
			<xs:extension base="ac:coreMarketItemType">
				<xs:sequence>
					<xs:element name="item" type="ac:externalItemType"
						minOccurs="1" maxOccurs="1" />
					<xs:element name="capabilityList">
						<xs:annotation>
							<xs:documentation>
								A list of elements which have a name attribute
								that describes
								which capability of the service that the market
								item offers.
							</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="capability" type="ac:capabilityType"
									minOccurs="0" maxOccurs="unbounded" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="itemType">
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The name of the Item. Ex:
						<B>AT&amp;T Extreme 6.0 DSL</B>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="providerId" type="xs:string" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The external id of the provider.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="providerName" type="xs:string"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The business name of the provider of the item. Ex:
						<B>AT&amp;T</B>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="itemCategory" type="ac:itemCategory"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="priceInfo" type="ac:priceInfoType"
				minOccurs="1" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>





	<xs:complexType name="itemCategory">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:annotation>
					<xs:documentation>
						Current possible values are:
						<OL>
							<LI>Bundles</LI>
							<LI>Electricity</LI>
							<LI>Local Phone</LI>
							<LI>Long Distance Phone</LI>
							<LI>Home Wire Protection</LI>
							<LI>Wireless Phone</LI>
							<LI>Cable TV</LI>
							<LI>Satellite TV</LI>
							<LI>High Speed Internet</LI>
							<LI>Dial Up Internet</LI>
							<LI>Local News Paper</LI>
							<LI>National News Paper</LI>
							<LI>Personal Checks</LI>
							<LI>Washer / Dryer Rental</LI>
							<LI>Warranty</LI>
							<LI>Home Security</LI>
							<LI>Waste Removal</LI>
							<LI>Natural Gas</LI>
							<LI>Energy Conservation</LI>
							<LI>Offers</LI>
							<LI>Second Call</LI>
							<LI>Water</LI>
							<LI>Appliance Protection</LI>
						</OL>
					</xs:documentation>
				</xs:annotation>
				<xs:attribute name="displayName" type="xs:string" use="optional">
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="externalItemType">
		<xs:complexContent>
			<xs:extension base="ac:itemType">
				<xs:sequence>
					<xs:element name="externalId" type="xs:string"
						minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The external id of the item.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="marketingHighlight" type="xs:string"
						minOccurs="0" maxOccurs="10">
						<xs:annotation>
							<xs:documentation>
								Textual bullet points that highlight aspects of the item that
								the service provider
								has chosen for marketing purposes.
								Ex:
								<B>"Blazing Download Speeds"</B>
								, or
								<B>"Static IP address included"</B>
								.
								Up to 10 instances of this element are possible in one item.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="capabilityType">
		<xs:attribute name="name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>
					Current possible values are:
					<OL>
						<LI>satellite</LI>
						<LI>analogCable</LI>
						<LI>digitalCable</LI>
						<LI>hdtv</LI>
						<LI>fiberDataDownSpeed</LI>
						<LI>fiberDataUpSpeed</LI>
						<LI>wiredDataUpSpeed</LI>
						<LI>wiredDataDownSpeed</LI>
						<LI>ipDslamVoip</LI>
						<LI>ipDslamIptv</LI>
						<LI>ipDslamDataDownSpeed</LI>
						<LI>ipDslamDataUpSpeed</LI>
						<LI>dialUpInternet</LI>
						<LI>electricity</LI>
						<LI>naturalGas</LI>
						<LI>localPhone</LI>
						<LI>longDistancePhone</LI>
						<LI>homeWireProtection</LI>
						<LI>wirelessPhone</LI>
						<LI>wasteRemoval</LI>
						<LI>localNewspaper</LI>
						<LI>nationalNewspaper</LI>
						<LI>homeSecurity</LI>
						<LI>water</LI>
						<LI>offers</LI>
						<LI>voip</LI>
						<LI>iptv</LI>
						<LI>warranty</LI>
						<LI>energyManagement</LI>
					</OL>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="channelType">
		<xs:sequence>
			<xs:element name="channel" type="xs:string" minOccurs="1"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="referrer" type="ac:businessPartyType" />




	<xs:complexType name="businessPartyType">

		<xs:all>
			<xs:element name="name" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="externalId" type="xs:string" minOccurs="1"
				maxOccurs="1" />
		</xs:all>

	</xs:complexType>

	<xs:complexType name="selectedFeaturesType">
		<xs:sequence>
			<xs:element name="features" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="featureValue" type="ac:featureValueType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>

				</xs:complexType>
			</xs:element>
			<xs:element name="featureGroup" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="featureValue" type="ac:featureValueType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
					<xs:attribute name="externalId" type="xs:string" />
					<xs:attribute name="groupType" default="0" type="xs:int">
						<xs:annotation>
							<xs:documentation>
								Represents the feature container type:
								-1 :
								PickAll
								0 : Unused
								1 : Pick One,
								N : PickN or PickUpToN
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>

				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="selectedDialogsType">
		<xs:sequence>
			<xs:element name="dialogs" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="dialog" type="ac:dialogValueType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>

				</xs:complexType>
			</xs:element>

		</xs:sequence>
	</xs:complexType>

	<xs:element name="dialog" type="ac:selectedDialogsType" />

	<xs:complexType name="dialogValueType">
		<xs:sequence>
			<xs:element name="externalId" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="dialogueDate" type="xs:dateTime"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="value" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="type">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="integer" />
										<xs:enumeration value="string" />
										<xs:enumeration value="boolean" />
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="selected" type="xs:boolean"
								default="true" />
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


<xs:complexType name="featureValueType">
		<xs:sequence>
			<xs:element name="price" type="ac:priceInfoType" minOccurs="0" maxOccurs="1" />

			<xs:element name="included" type="ac:emptyElementType"
				minOccurs="0" maxOccurs="1" />
			<xs:element name="required" type="xs:boolean" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="available" type="xs:boolean" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="doNotDisplay" type="ac:emptyElementType"
				minOccurs="0" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="externalId" type="xs:string" />
		<xs:attribute name="value" type="xs:string" />
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="integer" />
					<xs:enumeration value="string" />
					<xs:enumeration value="boolean" />
					<xs:enumeration value="group" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="description" type="xs:string" use="optional" />
		<xs:attribute name="tags" type="xs:string" use="optional" />
	</xs:complexType>




	<xs:element name="price" type="ac:priceInfoType" />
	<xs:element name="priceTierList" type="ac:priceTierListType" />

	<xs:complexType name="priceTierListType">
		<xs:sequence>
			<xs:element name="priceTier" type="ac:priceTierType"
				minOccurs="1" maxOccurs="10" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="priceTierType">
		<xs:sequence>
			<xs:element name="price" type="ac:priceInfoType" />
			<xs:element name="rangeStart" type="xs:int" />
			<xs:element name="rangeEnd" type="xs:int" minOccurs="0"
				maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="priceInfoType">
		<xs:annotation>
			<xs:documentation>
				This container contains all of the pricing
				information used by various
				entities.
				each contains a dollar amount
				representing the one-time price and the
				(typically monthly) recurring
				price of the service.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="baseNonRecurringPrice" type="xs:double"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The dollar amount representing the one-time
						price.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="baseNonRecurringPriceUnits" type="xs:string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The units representing any multiplier for the

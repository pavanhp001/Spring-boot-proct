				</xs:element>
				<xs:element name="integerConstraint">
					<xs:complexType>
						<xs:sequence>
							<xs:choice minOccurs="0" maxOccurs="1">
								<xs:sequence>
									<xs:element name="listOfValues" minOccurs="1"
										maxOccurs="1">
										<xs:complexType>

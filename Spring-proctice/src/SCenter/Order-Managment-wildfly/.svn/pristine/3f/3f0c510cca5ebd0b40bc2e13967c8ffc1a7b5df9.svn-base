COMMENT ON TABLE V_tx.om_rtim_order_mapping IS 'Store order mapping for real time products';

COMMENT ON COLUMN V_tx.om_rtim_order_mapping.id IS 'Primary key';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.V_order_no IS 'V Order number sent or received from real time provider like Dish,G2B,ATT etc';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.order_ext_id IS 'Reference to order external id';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.li_ext_id IS 'Reference to lineitem id';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.provider_ext_id IS 'Business party external id';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.dt_created IS 'Date when mapping was created';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.context IS 'Context info';
COMMENT ON COLUMN V_tx.om_rtim_order_mapping.description IS 'Description about mapping';
